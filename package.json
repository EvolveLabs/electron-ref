{
  "name": "electron-ref",
  "description": "This is a fork of the ref project, adding electron build support and binaries",
  "keywords": [
    "native",
    "buffer",
    "extensions",
    "c++",
    "pointer",
    "reference",
    "dereference",
    "type",
    "int",
    "long",
    "float",
    "double",
    "byte",
    "64"
  ],
  "version": "1.0.10",
  "author": "Justin Chase <justin@evolvelabs.com>",
  "repository": {
    "type": "git",
    "url": "git://github.com/EvolveLabs/electron-ref.git"
  },
  "main": "./lib/ref.js",
  "scripts": {
    "docs": "node docs/compile",
    "test": "mocha -gc --reporter spec",
    "install": "bash ./node_modules/.bin/elinst"
  },
  "engines": {
    "node": ">=2.2.1"
  },
  "binaries": [
    "https://s3.amazonaws.com/evolve-bin/{name}/{name}-{version}-{platform}-{arch}-{configuration}.tgz"
  ],
  "dependencies": {
    "bindings": "~1.2.0",
    "debug": "2",
    "electron-updater-tools": "~0.1.x"
  },
  "devDependencies": {
    "nan": "~1.8.4",
    "dox": "0.4.4",
    "highlight.js": "1",
    "jade": "0.35.0",
    "marked": "0.3.2",
    "mocha": "*",
    "electron-weak": "~0.4.8"
  },
  "bundledDependencies": [ "electron-updater-tools" ]
}
